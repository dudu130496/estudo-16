import math
ang = float(input('digite um angulo:'))
seno = math.sin(math.radians(ang))
cosseno = math.cos(math.radians(ang))
tangente = math.tan(math.radians(ang))
print('o angulo de {} tem o seno de {:.2f} \no angulo de {} tem o cosseno de {:.2f} \no angulo de {} tem a tangente de {:.2f}'.format(ang, seno, ang, cosseno, ang, tangente))
